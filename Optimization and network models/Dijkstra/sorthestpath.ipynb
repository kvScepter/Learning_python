    "import numpy as np\n",
    "import networkx as nx\n",
    "\n",
    "\n",
    "numpy_matriisi = np.genfromtxt('AA5360_kaupungit.csv', delimiter=',')\n",
    "G = nx.from_numpy_array(numpy_matriisi)\n",
    "reitti = nx.dijkstra_path(G, 6, 4)\n",
    "reitti_pituus = nx.dijkstra_path_length(G, 6, 4)\n",
    "\n",
    "\n",
    "print(reitti_pituus)\n",
    "print(reitti)\n",
    "\n",
    "\n",
    "\n"
   ]
